<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<StackPane styleClass="rootPane" alignment="center" xmlns="http://javafx.com/javafx/8.0.102-ea">
    <VBox alignment="center" styleClass="optionButtonContainer" id="vBox">
        <GridPane alignment="center" hgap="1" vgap="1" id="gameGrid"/>
        <Button contentDisplay="BOTTOM" styleClass="optionButton" text="%back"
                id="backBtn"/>
    </VBox>
    <stylesheets>
        <URL value="@/css/style.css"/>
    </stylesheets>
</StackPane>
